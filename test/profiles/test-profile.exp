set timeout 10
spawn $env(SHELL)
match_max 100000

if { $argc != 1 } {
	puts "TESTING ERROR: argument missing"
	puts "Usage: test-profile.exp profile_name"
	exit
}

send -- "firejail --profile=$argv echo done\r"
expect {
	timeout {puts "TESTING ERROR 0\n";exit}
	"done"
}
send -- "exit\r"
after 100
puts "\n"
